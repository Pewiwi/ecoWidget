public class NewsItem {
    public String title;
    public String subtitle;
    public String content;
    public String link;
    public String date;
    public String endDate;
    public String type;
    public String location;

    public NewsItem(String title, String subtitle, String content, String link, String date, String endDate, String type, String location) {
        this.title = title;
        this.subtitle = subtitle;
        this.content = content;
        this.link = link;
        this.date = date;
        this.endDate = endDate;
        this.type = type;
        this.location = location;
    }
}
